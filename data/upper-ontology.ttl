@prefix : <#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

:Food a rdfs:Class ;
    rdfs:label "Food" .
:Unit a rdfs:Class ;
    rdfs:label "Unit" .
:hasUnit a rdf:Property ;
    rdfs:label "has unit" ;
    rdfs:domain foo:Food ;
    rdfs:range foo:Unit .
